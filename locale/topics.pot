msgid "The age of session cookies, in seconds."
msgstr ""

#: ../../topics/http/sessions.txt:538
# 7bc616d4eee7434e9b6d3085046aaceb
msgid "SESSION_COOKIE_DOMAIN"
msgstr ""

#: ../../topics/http/sessions.txt:540
